{
  "name": "interactive-data-analysis-platform",
  "version": "1.0.0",
  "description": "An interactive platform for data analysis and visualization",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "cd frontend && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-all": "npm install && cd frontend && npm install",
    "build": "cd frontend && npm run build",
    "test": "jest --testPathPattern=backend && cd frontend && npm test",
    "lint": "eslint backend frontend"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.3.0",
    "helmet": "^4.6.0",
    "mongoose": "^5.13.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "xlsx": "^0.17.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "babel-jest": "^27.0.6",
    "concurrently": "^6.2.0",
    "eslint": "^7.30.0",
    "eslint-plugin-react": "^7.24.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.12"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/interactive-data-analysis-platform.git"
  },
  "author": "Your Name",
  "license": "MIT"
}
